{
  "name": "solid-firebase",
  "type": "module",
  "version": "0.2.3",
  "private": false,
  "packageManager": "pnpm@8.6.7",
  "description": "Solid primitives for Firebase.",
  "author": "wobsoriano",
  "license": "MIT",
  "contributors": [],
  "homepage": "https://github.com/wobsoriano/solid-firebase#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wobsoriano/solid-firebase.git"
  },
  "bugs": {
    "url": "https://github.com/wobsoriano/solid-firebase/issues"
  },
  "keywords": [
    "solid"
  ],
  "sideEffects": false,
  "exports": {
    "solid": {
      "development": "./dist/dev.jsx",
      "import": "./dist/index.jsx"
    },
    "development": {
      "require": "./dist/dev.cjs",
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/dev.js"
      }
    },
    "require": "./dist/index.cjs",
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "browser": {},
  "typesVersions": {},
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite serve dev",
    "test": "echo \"test not setup\"",
    "build": "tsup",
    "lint": "eslint .",
    "prepublishOnly": "pnpm build",
    "release": "bumpp && pnpm publish",
    "update-deps": "pnpm up -Li",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "firebase": ">=9.0.0",
    "solid-js": ">=1.6.0"
  },
  "devDependencies": {
    "@antfu/eslint-config-ts": "^0.39.7",
    "bumpp": "^9.1.1",
    "esbuild": "^0.18.12",
    "esbuild-plugin-solid": "^0.5.0",
    "eslint": "^8.44.0",
    "eslint-plugin-solid": "^0.12.1",
    "firebase": "^10.0.0",
    "solid-js": "^1.7.8",
    "tsup": "^7.1.0",
    "tsup-preset-solid": "^0.1.8",
    "typescript": "^5.1.6",
    "vite": "^4.4.3",
    "vite-plugin-solid": "^2.7.0"
  }
}
