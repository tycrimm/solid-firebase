{
  "version": "0.1.7",
  "name": "solid-firebase",
  "description": "Solid primitives for Firebase.",
  "license": "MIT",
  "author": "wobsoriano",
  "contributors": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wobsoriano/solid-firebase.git"
  },
  "homepage": "https://github.com/wobsoriano/solid-firebase#readme",
  "bugs": {
    "url": "https://github.com/wobsoriano/solid-firebase/issues"
  },
  "files": [
    "dist"
  ],
  "private": false,
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "browser": {},
  "exports": {
    "solid": {
      "development": "./dist/index.dev.solid.jsx",
      "import": "./dist/index.solid.jsx"
    },
    "browser": {
      "development": {
        "import": "./dist/index.dev.js",
        "require": "./dist/index.dev.cjs"
      },
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "development": {
      "import": "./dist/index.dev.js",
      "require": "./dist/index.dev.cjs"
    },
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "scripts": {
    "dev": "vite serve dev",
    "test": "echo \"test not setup\"",
    "build": "tsup",
    "lint": "eslint .",
    "prepublishOnly": "pnpm build",
    "format": "prettier --ignore-path .gitignore -w \"src/**/*.{js,ts,json,css,tsx,jsx}\" \"dev/**/*.{js,ts,json,css,tsx,jsx}\"",
    "release": "bumpp && pnpm publish",
    "update-deps": "pnpm up -Li",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "firebase": ">=9.0.0",
    "solid-js": ">=1.6.0"
  },
  "devDependencies": {
    "@antfu/eslint-config-ts": "^0.34.1",
    "bumpp": "^8.2.1",
    "esbuild": "^0.17.0",
    "esbuild-plugin-solid": "^0.4.2",
    "eslint": "^8.32.0",
    "eslint-plugin-solid": "^0.9.3",
    "firebase": "^9.15.0",
    "prettier": "2.8.3",
    "solid-js": "^1.6.9",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "vite-plugin-solid": "^2.5.0"
  },
  "keywords": [
    "solid"
  ],
  "packageManager": "pnpm@7.20.0"
}
